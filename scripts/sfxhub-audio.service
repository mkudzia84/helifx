[Unit]
Description=ScaleFX Hub Audio Level Setup
Documentation=https://github.com/yourusername/scalefx
DefaultDependencies=no
After=multi-user.target sound.target graphical.target systemd-user-sessions.service
Before=sfxhub.service
Wants=multi-user.target

[Service]
Type=oneshot
ExecStart=/usr/local/bin/sfxhub-audio-setup --verbose
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
